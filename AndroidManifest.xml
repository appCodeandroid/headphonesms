<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.smike.headphonesms" xmlns:android="http://schemas.android.com/apk/res/android" android:versionName="1.0" android:versionCode="2">

	<uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
	<uses-permission android:name="android.permission.READ_CONTACTS" />
	<uses-permission android:name="android.permission.RECEIVE_SMS" />
	<uses-sdk android:minSdkVersion="7"></uses-sdk>
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<receiver android:name=".HeadphoneSmsApp" android:enabled="true">
			<intent-filter>
				<action android:name="android.provider.Telephony.SMS_RECEIVED" />
			</intent-filter>
		</receiver>
		<service android:enabled="true" android:name=".ReadSmsService"></service>
	</application>
</manifest>